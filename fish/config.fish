if status is-interactive
  # Commands to run in interactive sessions can go here
  set fish_greeting ""
  set EDITOR "nvim"

  # Alias
  alias v "nvim"
  alias ls "exa"
  alias ll "exa -l"
  alias la "exa -a"
  alias cat "bat"


  set MOZ_ENABLE_WAYLAND 1
  set EDITOR "nvim"
  set TERM "alacritty"
end


