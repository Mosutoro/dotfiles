{
    "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
    "Lua.diagnostics.disable": [
        "redefined-local"
    ],
    "Lua.workspace.library": [
        "/home/me/.config/nvim",
        "/etc/xdg/nvim",
        "/home/me/.local/share/nvim/site",
        "/home/me/.local/share/nvim/site/pack/packer/start/mason-lspconfig.nvim",
        "/home/me/.local/share/nvim/site/pack/packer/start/mason.nvim",
        "/home/me/.local/share/nvim/site/pack/packer/start/nvim-lspconfig",
        "/home/me/.local/share/nvim/site/pack/packer/start/packer.nvim",
        "/usr/share/nvim/runtime",
        "/usr/lib/nvim",
        "/usr/share/vim/vimfiles",
        "${3rd}/luassert/library",
        "${3rd}/luv/library"
    ]
}